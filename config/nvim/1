return
{
  'nvimdev/dashboard-nvim',
  event = 'VimEnter',
  config = function()
    require('dashboard').setup {
      -- config
	theme = "hyper",
	 shortcut = {
    -- action can be a function type
    { desc = string, group = 'highlight group', key = 'shortcut key', action = 'action when you press key' },
  },
    }
  end,
  dependencies = { {'nvim-tree/nvim-web-devicons'}}
}
